        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF(NOT(ISPICKVAL(Type_of_Student__c, &quot;Graduate Student&quot;)), 
If(MONTH(TODAY())&lt; VALUE($Label.jcampuconnect__dayofmonthtoswitch), 

CASE(Expected__c , 
Year(Today()), &quot;Senior&quot;, 
Year(Today())+1, &quot;Junior&quot;, 
Year(Today())+2, &quot;Sophomore&quot;, 
Year(Today())+3, &quot;Freshman&quot;, 
&quot;&quot;), 

CASE(Expected__c, 
Year(Today())+1, &quot;Senior&quot;, 
Year(Today())+2, &quot;Junior&quot;, 
Year(Today())+3, &quot;Sophomore&quot;, 
Year(Today())+4, &quot;Freshman&quot;, 
&quot;&quot;)), 

&quot;&quot;)</formula>
